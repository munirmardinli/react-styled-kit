  "keywords": [
    "styled",
    "design",
    "UI",
    "Munir Mardinli"
  ],
  "bugs": {
    "url": "https://gitlab.com/opensourceCodde/metamorphosis-fullstack/-/issues",
    "email": "munir@mardinli.de"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
