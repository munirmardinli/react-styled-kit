    "@mui/styles": "^6.4.5",
    "@mui/x-data-grid-pro": "^8.5.2",
    "@mui/x-date-pickers-pro": "^6.18.4",
    "dotenv": "^16.4.7",
    "next": "^15.2.4",
    "react": "^19",
    "react-big-calendar": "^1.17.1",
    "react-datepicker": "^8.4.0"
  },
  "devDependencies": {
    "@types/node": "^24.0.3",
