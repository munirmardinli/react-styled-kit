{
  "name": "@munirmardinli_org/react-styled-kit",
  "version": "4.0.4",
  "description": "A modern React UI kit with styled-components, Material-UI, and powerful data grids. Perfect for building responsive dashboards, admin panels, and enterprise applications.",
  "private": false,
  "sideEffects": false,
  "license": "MIT",
  "keywords": [
    "styled",
    "design",
    "UI",
    "Munir Mardinli"
  ],
  "bugs": {
    "url": "https://github.com/munirmardinli/react-styled-kit/-/issues",
    "email": "munir@mardinli.de"
  },
  "homepage": "https://github.com/munirmardinli/react-styled-kit#readme",
  "author": {
    "name": "Munir Mardinli",
    "email": "munir@mardinli.de",
    "url": "https://github.com/munirmardinli"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/munirmardinli/react-styled-kit.git",
    "directory": "packages/core"
  },
  "files": [
    "lib"
  ],
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "require": "./lib/index.js"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "funding": {
    "type": "individual",
    "url": "https://linktr.ee/munirmardinli"
  },
  "contributors": [
    {
      "name": "Munir Mardinli",
      "email": "Munir@Mardinli.de",
      "url": "https://linktr.ee/munirmardinli"
    }
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "run-script-os",
    "build:win32": "tsc && npm run copy-css",
    "build:linux": "tsc && npm run copy-css",
    "build:darwin": "tsc && npm run copy-css",
    "copy-css": "copyfiles -u 1 \"src/**/*.css\" lib",
    "storybook": "storybook dev -p 6006"
  },
  "dependencies": {
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^7.1.1",
    "@mui/material": "^7.1.1",
    "@mui/styles": "^6.4.5",
    "@mui/x-data-grid-pro": "^8.5.2",
    "@mui/x-date-pickers-pro": "^8.5.2",
    "next": "^15.2.4",
    "react-big-calendar": "^1.17.1",
    "react-datepicker": "^8.4.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.0.1",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.3",
    "@semantic-release/release-notes-generator": "^14.0.3",
    "@storybook/addon-a11y": "^9.0.12",
    "@storybook/addon-docs": "^9.0.12",
    "@storybook/addon-themes": "^9.0.12",
    "@storybook/addon-vitest": "^9.0.12",
    "@storybook/nextjs-vite": "^9.0.12",
    "@svgr/rollup": "^8.1.0",
    "@types/node": "^24.0.3",
    "@types/react": "^19.1.8",
    "@types/react-big-calendar": "^1.16.2",
    "@types/react-dom": "^19.1.6",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "commitizen": "^4.3.1",
    "conventional-changelog-conventionalcommits": "^9.0.0",
    "copyfiles": "^2.4.1",
    "cz-conventional-changelog": "^3.3.0",
    "date-fns": "^4.1.0",
    "husky": "^9.1.7",
    "playwright": "^1.53.1",
    "run-script-os": "^1.1.6",
    "semantic-release": "^24.2.5",
    "storybook": "^9.0.12",
    "ts-node": "^10.9.1",
    "typedoc": "^0.28.5",
    "typescript": "^5.7.3",
    "vitest": "^3.2.4"
  },
  "tsdocsOptions": {
    "changelog": true
  }
}
